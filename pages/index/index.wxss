/**index.wxss**/
.home {
  width:100%;
}
.banner {
  width:100%;
  height: 380rpx;
}
.banner>swiper-item>image {
  width: 750rpx;
  height: 380rpx;
}
.nav {
  display: flex;
  width: 100%;
  background: #fff;
  flex-wrap:wrap;
}
.nav navigator  {
  display: flex;
  box-sizing: border-box;
  width: 250rpx;
  height: 250rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-right: 1px solid #ebebeb;
  border-bottom: 1px solid #ebebeb
}
.nav navigator image {
  width: 80rpx;
  height: 80rpx;
}
.nav navigator text {
  display: block;
  font-size: 28rpx;
  color: #242424;
}
.footer {
  display: flex;
  width:100%;
  background: #fff;
  margin-top: 10rpx;
}
.footer>image {
  height: 180rpx;
  margin: 0 40rpx;
  margin-top: 10rpx;
}